{
  "name": "frp-spotify",
  "version": "1.0.0",
  "description": "A node app utilizing spotify api",
  "main": "server.js",
  "scripts": {
    "start": "npm-run-all --parallel lint:watch open:src",
    "open:src": "nodemon ./server/server.js --exec babel-node",
    "lint": "esw ./**/*.js",
    "lint:watch": "npm run lint -- --watch"
  },
  "keywords": [
    "node",
    "spotify",
    "functional programming",
    "express"
  ],
  "author": "vinnyA3",
  "license": "MIT",
  "dependencies": {
    "accounting": "^0.4.1",
    "data.either": "^1.5.1",
    "data.task": "^3.1.1",
    "express": "^4.15.3",
    "pug": "^2.0.0-rc.2",
    "ramda": "^0.24.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.0",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "webpack": "^2.6.1"
